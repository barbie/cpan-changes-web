<h2><% params.dist %></h2>
<div id=status>
    <h3>Status</h3>
    <dl>
        <dt>Releases</dt>
        <dd><% releases %></dd>
        <dt>Pass</dt>
        <dd><% pass %></dd>
        <dt>Fail</dt>
        <dd><% fail %></dd>
        <dt class=progress>Progress</dt>
        <dd class=progress><% progress %>%</dd>
    </dl>
</div>
<ul id=releases>
<% WHILE( release = releases.next ) %>
    <li><% release.distribution %> <% release.version %> by <a href="<% author_uri %>/<% release.author %>"><% release.author %></a> (<% IF !release.failure %>PASS)<% ELSE %>FAIL)<br><span class=error><% release.failure %></span><% END %></li>
<% END %>
</ul>
