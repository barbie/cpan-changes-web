<h2>Distributions</h2>

<form name="search" action="/search" method='post'>
    <input type="search" id="search" name="search" placeholder="Search module" />
    <input type="hidden" id="search_module" name="search_module" value="1" />
</form>

<ul>
<% FOREACH dist IN distributions %>
    <li><a href="<% dist_uri %>/<% dist %>"><% dist %></a></li>
<% END %>
</ul>
